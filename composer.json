{
    "name": "inpsyde/more-menu-fields",
    "description": "Package to add more fields to WordPress menu edit screen.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Inpsyde GmbH",
            "email": "hello@inpsyde.com",
            "homepage": "http://inpsyde.com",
            "role": "Company"
        },
        {
            "name": "Giuseppe Mazzapica",
            "email": "g.mazzapica@inpsyde.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7"
    },
    "require-dev": {
        "phpunit/phpunit": "6.3.*",
        "doctrine/instantiator": "~1.0.5",
        "brain/monkey": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Inpsyde\\MoreMenuFields\\": "src/"
        },
        "files": [
            "inc/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Inpsyde\\MoreMenuFields\\Tests\\": "tests/src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}